class Level extends Scene{
  String level_name;
  int[][] layout;
  
  Level(String level_name, int[][] layout,Scene previous, Scene next) {
    super(level_name, previous, next);  //<>//
    this.level_name = level_name;
    this.layout = layout;
  }
  
  void draw(){
    for(int row = 0; row < this.layout.length; row++) {
      for(int col = 0; col < this.layout[row].length; col++) {
        int id = this.layout[row][col];
        noStroke();
        switch(id) {
            case 0:
                fill(100, 150, 175);
                rect(col*20,row*20, 20, 20);
                break;
            case 1:
                fill(100,100, 100);
                rect(col* 20,row * 20, 20, 20);
                break;
            case 2:
                fill(255, 0, 0);
                rect(col * 20, row * 20, 20, 20);
                break;
        }
      }
    }
  }
}
